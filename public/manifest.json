{
  "manifest_version": 3,
  "name": "Chrome Extension with React",
  "description": "A chrome extension boilerplate built with create-react-app",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo192.png"
  },
  "icons": {
    "128": "logo512.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["logo192.png", "logo512.png"],
      "matches": []
    }
  ]
}
